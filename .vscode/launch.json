{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "debug chatterbox",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/chatterbox",
      "args": ["-p", "-v", "inf"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/wd",
      "environment": [
      ],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          }
      ],
    }
  ]
}
